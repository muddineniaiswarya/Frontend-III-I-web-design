    <h1>non primitive datatypes</h1>
    <script>
        //object
        let person={
        name:'john'
        }
        console.log(typeof person);
        //array
        let numbers=[1,2,3,4];
        console.log(typeof numbers);
        console.log(Array.isArray(numbers));
        //function
        function greet(){
            return hello;
        }
        console.log(typeof greet);
    </script>
